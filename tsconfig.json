{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@public/*": [
        "public/*"
      ]
    },
    "types": [
      "vitest/importMeta",
    ],
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      },
    ],
  }
}